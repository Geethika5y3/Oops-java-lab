package Com.y3;

public class Conditional {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int a=4;
      int b=6;
      if(a>b)
        {
    	  System.out.println("a is greater than b");
        }
      else if(a<b)
      {
    	  System.out.println(" a is less than b");
      }
	}

}
OUTPUT:
 a is less than b
